separator=false
separator_block_width=5
border_top=0
border_left=0
border_right=0
border_bottom=4

[volume]
label=  
command=bash ~/.config/scripts/volume
interval=1
border=#cc99cc

[wireless]
label=    
command=[[ -z "$(iwgetid -r)" ]] || echo "$(iwgetid -r) "
interval=2
border=#66cccc

[time]
label=    
command=echo "`date +'%a %d %b'` <b>`date +'%T'`</b> "; [[ -z "${BLOCK_BUTTON}" ]] || gsimplecal &
interval=1
markup=pango
border=#99cc99
separator_block_width=0
